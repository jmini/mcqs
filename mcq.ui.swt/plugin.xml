<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="mcq.ui.swt.perspective.Perspective"
            fixed="true"
            id="mcq.ui.swt.perspective.Perspective"
            name="MCQ"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="mcq.ui.swt.views.CenterView"
            icon="resources/icons/eclipse_scout.gif"
            id="mcq.ui.swt.views.CenterView"
            name="CenterView">
      </view>
      <view
            class="mcq.ui.swt.views.OutlinePageView"
            icon="resources/icons/eclipse_scout.gif"
            id="mcq.ui.swt.views.OutlinePageView"
            name="Outline"
            restorable="true">
      </view>
      <view
            class="mcq.ui.swt.views.TablePageView"
            icon="resources/icons/eclipse_scout.gif"
            id="mcq.ui.swt.views.TablePageView"
            name="Table Page"
            restorable="true">
      </view>
      <view
            class="mcq.ui.swt.views.SearchView"
            icon="resources/icons/eclipse_scout.gif"
            id="mcq.ui.swt.views.SearchView"
            name="Search"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="mcq.ui.swt.SwtStartup">
      </startup>
   </extension>
     <extension id="application" name="Application" point="org.eclipse.core.runtime.applications">
    <application>
      <run class="mcq.ui.swt.application.Application"/>
    </application>
  </extension>
  <extension id="product" point="org.eclipse.core.runtime.products">
    <product
          application="mcq.ui.swt.application"
          name="MCQ">
       <property
             name="appName"
             value="MCQ"/>
       <property
          name="windowImages"
          value="resources/icons/eclipseScout16x16.gif,resources/icons/eclipseScout32x32.gif"/>
    </product>
  </extension>
  <extension
      point="org.eclipse.ui.views">
   <view
         class="org.eclipse.ui.internal.progress.ProgressView"
         icon="resources/icons/progress_none.gif"
         id="org.eclipse.ui.views.ProgressView"
         name="Progress View">
   </view>
  </extension>

</plugin>
